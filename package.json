{
  "name": "photocmd",
  "version": "0.0.3",
  "description": "a command-line tool for working with ‘families’ of photos, syncing metadata, deleting and renaming",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "babel src -d build -s",
    "watch": "babel src -d build -s -w ",

    "test": "mocha ./test/**/*.spec.js",
    "test-watch": "mocha -R list -w ./test/**/*.spec.js",
    "test-single": "mocha -- $1",
    "test-single-watch": "mocha -R list -w -- $1",

    "paramtest": "echo $([ -z $1 ] && echo 42 || echo $1)",
    "test-and-build": "npm run build && npm run test",
    "start": "watch 'npm run test-and-build' ./src/ ./test/ --interval=.2"
  },
  "keywords": [
    "photo",
    "raw",
    "jpeg",
    "tagging",
    "deletion",
    "metadata",
    "stamping"
  ],
  "author": "Frank",
  "license": "Apache-2.0",
  "bin": {
    "photo": "./build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nocke/photocmd.git"
  },
  "bugs": {
    "url": "https://github.com/nocke/photocmd/issues"
  },
  "homepage": "https://github.com/nocke/photocmd#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "commander": "^2.9.0",
    "core-js": "^2.4.1",
    "trash": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "sinon": "^4.0.1",
    "watch": "^1.0.2"
  }
}
