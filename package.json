{
  "name": "photocmd",
  "version": "0.0.5",
  "description": "a command-line tool for working with ‘families’ of photos, syncing metadata, deleting and renaming",
  "main": "build/index.js",
  "scripts": {
    "ui": "electron build/photoui.js",
    "start": "watch 'npm run test-and-build' ./src/ ./test/ --interval=.2",
    "test-and-build": "npm run build && npm run test",
    "clean": "rm -rf build && mkdir build",
    "build": "babel src -d build -s",
    "watch": "babel src -d build -s -w ",
    "test": "mocha ./test/**/*.test.js",
    "test-watch": "mocha -R list -w ./test/**/*.test.js",
    "test-single": "mocha $1",
    "test-single-watch": "mocha -R list -w $1",
    "test-live": "unzip -o sample2.zip -d build/commandTests/  &&  photo del ./build/commandTests/sample2/ -lv",
    "paramtest": "echo $([ -z $1 ] && echo 42 || echo $1)",
	"beautify": "scripts/beautify.sh",
	"beautify2": "find . -regextype egrep -regex './(src|test|app)/.*\\.(js|sass|html)' -print0 | xargs -0 js-beautify -r"
  },
  "keywords": [
    "photo",
    "raw",
    "jpeg",
    "tagging",
    "deletion",
    "metadata",
    "stamping"
  ],
  "author": "Frank",
  "license": "Apache-2.0",
  "bin": {
    "photo": "./build/index.js",
    "photoui": "./build/photoui.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nocke/photocmd.git"
  },
  "bugs": {
    "url": "https://github.com/nocke/photocmd/issues"
  },
  "homepage": "https://github.com/nocke/photocmd#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "commander": "^2.16.0",
    "core-js": "^2.4.1",
    "trash": "^4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.24.4",
    "chai": "^3.5.0",
    "cross-env": "^5.1.6",
    "del": "^3.0.0",
    "electron": "^1.7.12",
    "extract-zip": "^1.6.7",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-livereload": "^3.8.1",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^5.0.0",
    "js-beautify": "^1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.3",
    "watch": "^1.0.2"
  }
}
